{
   "siteName": "Bitcot Technologies",
    "siteRoot": "http://bitcot.herokuapp.com",
    "siteIP": "0.0.0.0",
    "log_level": "info",
     "db": {
        "type":"mongo",
        "servers": [
          "mongodb://dogen.mongohq.com:10083/bitcot-test/"
        ],
        "name": "bitcot-test",
        "authentication": {
            "un": "bitcot",
            "pw": "bitcot"
        },
        "writeConcern": 1
    },
    "cache": {
        "fake": true,
        "host": "localhost",
        "port": 6379
    },
    "settings": {
        "use_memory": false,
        "use_cache": false
    },
    "templates": {
        "use_memory": true,
        "use_cache": false
    },
    "plugins": {
        "caching": {
            "use_memory": false,
            "use_cache": false
        }
    }
}
